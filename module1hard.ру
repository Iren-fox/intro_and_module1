grades = [[5, 3, 3, 5, 4], [2, 2, 2, 3], [4, 5, 5, 2], [4, 4, 3], [5, 5, 5, 4, 5]]
students = {'Johnny', 'Bilbo', 'Steve', 'Khendrik', 'Aaron'}
a0=sum(list(grades[0]))/len(grades[0])
a1=sum(list(grades[1]))/len(grades[1])
a2=sum(list(grades[2]))/len(grades[2])
a3=sum(list(grades[3]))/len(grades[3])
a4=sum(list(grades[4]))/len(grades[4])
list_sort=sorted(list(students))
result={list_sort[0]:a0,list_sort[1]:a1,list_sort[2]:a2,list_sort[3]:a3,list_sort[4]:a4}
print(result)